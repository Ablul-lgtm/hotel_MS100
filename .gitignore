CREATE TABLE BookingMs (
    BookingID INT PRIMARY KEY,
    HotelID INT,
    RoomID INT,
    GuestName VARCHAR(100),
    CheckInDate DATE,
    CheckOutDate DATE,
    TotalAmount DECIMAL(10, 2),
    FOREIGN KEY (HotelID) REFERENCES HotelMs(HotelID),
    FOREIGN KEY (RoomID) REFERENCES RoomMs(RoomID)
);
