CREATE TABLE HotelMs (
    HotelID INT PRIMARY KEY,
    HotelName VARCHAR(100) NOT NULL,
    Address VARCHAR(255),
    City VARCHAR(100),
    State VARCHAR(100),
    ZipCode VARCHAR(20),
    Country VARCHAR(100),
    PhoneNumber VARCHAR(20),
    Email VARCHAR(100),
    Rating DECIMAL(2, 1)
)
